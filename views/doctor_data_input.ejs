    <h1 style="color: white;">Doctor Data Input</h1>
    <form action="/save-doctor-data" method="POST" enctype="multipart/form-data">
        <!-- Existing form fields -->
        <label class="lbl" for="name">Name:</label>
        <input class="in" type="text" id="name" name="name" value="<%= doctor ? doctor.name : '' %>" required>
        <br>
        <label class="lbl" for="pmdc">PMDC Number:</label>
        <input class="in" type="text" id="pmdc" name="pmdc" value="<%= doctor ? doctor.pmdc : '' %>" required>
        <br>
        <label class="lbl" for="qualification">Qualification:</label>
        <input class="in" type="text" id="qualification" name="qualification" value="<%= doctor ? doctor.qualification : '' %>" required>
        <br>
        <label class="lbl" for="startTime">Start Time:</label>
        <input class="in" type="text" id="startTime" name="startTime" value="<%= doctor ? doctor.startTime : '' %>" required>
        <br>
        <label class="lbl" for="endTime">End Time:</label>
        <input class="in" type="text" id="endTime" name="endTime" value="<%= doctor ? doctor.endTime : '' %>" required>
        <br>
        <label class="lbl" for="specialization">Specialization:</label>
        <select class="in" id="specialization" name="specialization" required>
            <option value="" disabled selected>Select Specialization</option>
            <option value="Cardiology" <%= doctor && doctor.specialization === 'Cardiology' ? 'selected' : '' %>>Cardiology</option>
            <option value="Pulmonary" <%= doctor && doctor.specialization === 'Pulmonary' ? 'selected' : '' %>>Pulmonary</option>
            <option value="Neurology" <%= doctor && doctor.specialization === 'Neurology' ? 'selected' : '' %>>Neurology</option>
            <option value="Orthopedics" <%= doctor && doctor.specialization === 'Orthopedics' ? 'selected' : '' %>>Orthopedics</option>
        </select>
        <br>
        <label class="lbl" for="socialMedia1">Social Media Link 1:</label>
        <input class="in" type="url" id="socialMedia1" name="socialMedia1" value="<%= doctor ? doctor.socialMedia1 : '' %>">
        <br>
        <label class="lbl" for="socialMedia2">Social Media Link 2:</label>
        <input class="in" type="url" id="socialMedia2" name="socialMedia2" value="<%= doctor ? doctor.socialMedia2 : '' %>">
        <br>
        <label class="lbl" for="photo">Photo:</label>
        <input type="file" id="photo" name="photo">
        <br>
        <!-- New appointment fee input -->
        <label class="lbl" for="appointmentFee">Appointment Fee:</label>
        <input class="in" type="number" id="appointmentFee" name="appointmentFee" value="<%= doctor ? doctor.appointmentFee : '' %>" required>
        <br>
        <button class="sb" type="submit">Save Data</button>
    </form>
    
    

